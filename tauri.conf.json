{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Shiroyaku",
  "version": "0.1.0",
  "identifier": "com.Shiroyaku.app",
  "build": {
    "frontendDist": "dist"
  },
  "app": {
    "windows": [
      {
        "title": "Shiroyaku - Medical Symptom Search",
